}

/* Sub Level Nav */
ul.sexy-dropdown li ul {
	background: #F4F4F4;
	border: 1px solid #333;
		-webkit-border-radius: 5px;
		-moz-border-radius: 5px;
	-webkit-box-shadow: 0px 0px 3px rgba(0, 0, 0, 0.5);
	margin: 10px 0 0 -46px;
	padding: 0;
	position: relative;
}
ul.sexy-dropdown li ul li.selected {
    background: #888;
}
ul.sexy-dropdown li ul li {
    color: #333;
	border-bottom: 1px solid #CCC;
	display: block;
	float: none;
	padding: 8px 10px;
	width: 526px;
	margin: 0;
        cursor: pointer;
}
ul.sexy-dropdown li ul li:hover {
    background: #aaa;
}


/* Arrow */
ul.sexy-dropdown li.arrow {
	background: url(../images/arrow.png) no-repeat;
	border: 0;
	display: none;
	position: absolute;
		top: -10px;
		left: 63px;
	height: 11px;
	width: 20px;
	text-indent: -9999px;
}






